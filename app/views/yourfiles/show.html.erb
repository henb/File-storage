<%- model_class = Yourfile -%>
<div class="page-header">
  <h1>Your File</h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:file_id) %>:</strong></dt>
  <dd><%= @yourfile.file_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:file_name) %>:</strong></dt>
  <dd><%= @yourfile.file_file_name %></dd>
  <dt><strong><%= model_class.human_attribute_name(:file_content_type) %>:</strong></dt>
  <dd><%= @yourfile.file_content_type %></dd>
  <dt><strong><%= model_class.human_attribute_name(:file_size) %>:</strong></dt>
  <dd><%= @yourfile.file_file_size %></dd>
  <dt><strong><%= model_class.human_attribute_name(:file_updated_at) %>:</strong></dt>
  <dd><%= @yourfile.file_updated_at %></dd>
  <dt><strong><%= model_class.human_attribute_name(:file_fingerprint) %>:</strong></dt>
  <dd><%= @yourfile.file_fingerprint %></dd>
  <dt><strong><%= model_class.human_attribute_name(:user_id) %>:</strong></dt>
  <dd><%= @yourfile.user.email %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              yourfiles_path, :class => 'btn'  %>
</div>
